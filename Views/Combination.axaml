<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:controls="clr-namespace:MedEye.Controls"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="MedEye.Views.Combination"
        Title="Combination"
        WindowState="FullScreen"
        Background="Black">
  <Canvas>
    <controls:DragControl 
      Background="Red"
      Height="50"
      Width="50"
      Name="FirstObject"
      ZIndex="2">
    </controls:DragControl>
    <controls:DragControl
      Background="Blue"
      Height="150"
      Width="150"
      Name="SecondObject"
      Canvas.Left="200"
      Canvas.Top="200"
      CornerRadius="75">
      <Border
        Background="Black"
        Height="125"
        Width="125"
        CornerRadius="62.5">
      </Border>
    </controls:DragControl>
    <Label Background="White" Height="100" Width="300" Name="Log"></Label>
  </Canvas>
</Window>
