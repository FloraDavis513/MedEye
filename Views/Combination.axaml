<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:controls="clr-namespace:MedEye.Controls"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="MedEye.Views.Combination"
        Title="Combination"
        WindowState="FullScreen"
        Background="Black">
  <Canvas>
    <controls:DragControl 
      Background="Red"
      Height="100"
      Width="100"
      Name="FirstObject"
      ZIndex="2">
    </controls:DragControl>
    <controls:DragControl
      Background="Blue"
      Height="350"
      Width="350"
      Name="SecondObject"
      CornerRadius="175">
      <Border
        Background="Black"
        Height="300"
        Width="300"
        CornerRadius="150">
      </Border>
    </controls:DragControl>
    <Border
        Background="White"
        Height="150"
        Width="500"
        CornerRadius="15"
        Name="Log"
        Opacity="0">
      <Label
      Background="White"
      Height="140"
      Width="450"
      ZIndex="3"
      FontSize="30">Отклонение:
      </Label>
    </Border>
    
  </Canvas>
</Window>
